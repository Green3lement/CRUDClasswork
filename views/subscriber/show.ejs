<h2 class="center"> Subscriber Data for <%= subcriber.name %></h2>
<table class="table">
    <tbody>
        <% subscribers.forEach(subscriber => {%
            <tr>
                <td>Name</td>
                <td>
                    <%= subcriber.name %>
                </td>
            </tr>
            <tr>
                <td>Email</td>
                <td>
                    <%= subcriber.email %>
                </td>
            </tr>
            <tr>
                <td>Zip Code</td>
                <td>
                    <%= subcriber.zipCode %>
                </td>
            </tr>
            <tr>
                <td>Number of courses</td>
                <td>
                    <%= subcriber.courses.length %>
                </td>
            </tr>
    </tbody>
</table>